{
  "name": "ai-editor",
  "version": "0.1.0",
  "private": true,
  "main": "electron-start.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:next": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "dev:all": "concurrently \"npm run dev:next\" \"npm run dev:electron\" \"npm run dev:yjs\"",
    "package": "electron-builder build --dir",
    "dist": "npm run build && electron-builder",
    "lint": "next lint",
    "dev:yjs": "node yjs-server.js"
  },
  "build": {
    "appId": "com.aieditor.app",
    "productName": "AI Editor",
    "files": [
      "out/**/*",
      "electron-main.ts",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis",
      "icon": "public/favicon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/favicon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/favicon.ico"
    }
  },
